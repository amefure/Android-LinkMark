<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="white">#FFFFFFFF</color>
    <color name="ex_blue">#ADC2C8</color>
    <color name="ex_green">#ADC8C2</color>
    <color name="ex_light_gray">#dedede</color>
    <color name="ex_negative">#AB6F6F</color>
    <color name="ex_positive">#6F8EAB</color>
    <color name="ex_purple">#BEADC8</color>
    <color name="ex_red">#C8ADAD</color>
    <color name="ex_text">#545454</color>
    <color name="ex_thema">#E8E8E5</color>
    <color name="ex_yellow">#C4C8AD</color>

    <!-- ActionBarの背景色 -->
    <color name="colorPrimary">@color/ex_text</color>
    <!-- ステータスバーの背景色 -->
    <color name="colorPrimaryDark">@color/ex_text</color>
